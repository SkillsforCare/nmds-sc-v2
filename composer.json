{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "chumper/zipper": "^1.0",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.0",
        "nunomaduro/collision": "^2.0",
        "spatie/laravel-binary-uuid": "^1.1",
        "spatie/laravel-medialibrary": "7.0.0",
        "spatie/laravel-permission": "^2.12"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "files": [
          "htdocs/app/helpers.php"
        ],
        "classmap": [
          "htdocs/database/seeds",
          "htdocs/database/factories"
        ],
        "psr-4": {
            "App\\": "htdocs/app"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "htdocs/tests"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php htdocs/artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php htdocs/artisan package:discover",
            "@php htdocs/artisan database:migrate-environment"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "vendor-dir": "htdocs/vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
